Signed-off-by: Igal Chernobelsky <igalc@ti.com>
---
 mac80211/config/hostapd.conf |    4 ++--
 1 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/mac80211/config/hostapd.conf b/mac80211/config/hostapd.conf
index f2a85da..4205581 100644
--- a/mac80211/config/hostapd.conf
+++ b/mac80211/config/hostapd.conf
@@ -29,8 +29,8 @@ hw_mode=g
 channel=11
 beacon_int=100
 dtim_period=2
-supported_rates=60 120 240 360 480 540
-basic_rates=60 120 240
+supported_rates=10 20 55 110 60 120 240 360 480 540
+basic_rates=10 20 55 110 60 120 240
 preamble=1
 ignore_broadcast_ssid=0
 wpa_group_rekey=0
